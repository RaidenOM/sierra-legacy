{
  "expo": {
    "name": "Sierra Legacy",
    "slug": "sierra-legacy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sierra.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/sierra.png",
      "resizeMode": "center",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/sierra.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sierra.legacy.app",
      "permissions": [
        "READ_CONTACTS",
        "WRITE_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/sierra.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "3a3fad23-5f40-4eff-9cf0-584dfc7459b2"
      }
    }
  }
}